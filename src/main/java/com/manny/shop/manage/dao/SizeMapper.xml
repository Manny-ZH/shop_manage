<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "//mybaits.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.manny.shop.manage.dao.SizeMapper">
    <insert id="saveOne" useGeneratedKeys="true">
        INSERT INTO tbl_size(size_name,parent_id,status,remark)
        VALUE (#{sizeName},#{parentId},#{status},#{remark})
    </insert>
</mapper>